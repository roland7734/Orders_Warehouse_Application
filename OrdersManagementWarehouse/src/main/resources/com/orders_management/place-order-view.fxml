<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="644.0" prefWidth="818.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.orders_management.gui.PlaceOrderViewController">
   <children>
      <TableView fx:id="tableView" layoutX="72.0" layoutY="109.0" prefHeight="426.0" prefWidth="686.0" />
      <Button fx:id="cancelButton" layoutX="163.0" layoutY="566.0" mnemonicParsing="false" onAction="#onCancelButtonClick" styleClass="operation-button" stylesheets="@styles.css" text="Cancel">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="placeOrderButton" layoutX="512.0" layoutY="566.0" mnemonicParsing="false" onAction="#onPlaceOrderButtonClick" styleClass="operation-button" stylesheets="@styles.css" text="Place an Order">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Button>
      <ComboBox fx:id="comboBox" layoutX="72.0" layoutY="47.0" prefHeight="26.0" prefWidth="305.0" promptText="Choose a Customer" />
   </children>
</AnchorPane>
